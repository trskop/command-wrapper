-- vim: filetype=dhall

let script =
      { url =
          "https://raw.githubusercontent.com/trskop/command-wrapper/0.1.0.0-rc8/command-wrapper/dhall/Exec/bazel/completion-script"
      , hash =
          "sha256:732b498c252fe00f55da10780ca9b31ae0b56237741c97e778313556ec26e027"
      }

let wrapper =
        ../completion/wrapper sha256:2581cfe8c184898fe6b04aedad5ad9c1be34154b3a795ecab17ee19c899e5c8b
      ? ../completion/wrapper

in    λ(toolset : Text)
    → wrapper
        { toolset = toolset
        , expression = "${script.url} ${script.hash}"
        , command = "bazel"
        }
