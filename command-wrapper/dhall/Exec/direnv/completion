-- vim: filetype=dhall

let script =
      { url = "https://raw.githubusercontent.com/trskop/command-wrapper/0.1.0.0-rc8/command-wrapper/dhall/Exec/direnv/completion-script"
      , hash = "sha256:806c6f8632ddddedfd3c93330f26f074078bc26c4ffd91a232e589385f198c51"
      }

let wrapper =
        ../completion/wrapper sha256:2581cfe8c184898fe6b04aedad5ad9c1be34154b3a795ecab17ee19c899e5c8b
      ? ../completion/wrapper

in    λ(toolset : Text)
    → wrapper
        { toolset = toolset
        , expression = "${script.url} ${script.hash}"
        , command = "direnv"
        }
