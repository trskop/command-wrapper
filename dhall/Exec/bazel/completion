-- vim: filetype=dhall

let script =
      { url =
          "https://raw.githubusercontent.com/trskop/command-wrapper/f1d0363570f96f476ec68366f10f742d2702e845/dhall/Exec/completion/bazel.bash"
      , hash =
          "sha256:4263b8d2d363f446e849fafddc469d046eecee666d026ba306ed29b65973cfa6"
      }

in    λ(toolset : Text)
    → ../completion/wrapper
        { toolset = toolset
        , expression = "${script.url} ${script.hash} as Text"
        , command = "bazel"
        }
