-- vim: filetype=dhall

let script =
        ../completion/scripts.dhall sha256:501944afd38c5bc8bfed78c6195f80a7c804eb5ee2e97489b6b109f267c5ed91
      ? ../completion/scripts.dhall

let wrapper =
        ../completion/wrapper sha256:6f4115a040ee81275a40a8785baefbe391cf67cb57e5d4e79a8e0fedf572c036
      ? ../completion/wrapper

in    λ(toolset : Text)
    → wrapper
        { toolset = toolset
        , expression = "(${script.url} ${script.hash}).bazel"
        , command = "bazel"
        }
