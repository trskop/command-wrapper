let script =
      { url =
          "https://raw.githubusercontent.com/trskop/command-wrapper/master/dhall/Exec/completion/bazel.bash"
      , hash =
          "sha256:eab9655a19a4a35cf0b92b4c4cf310b05130bb45b19858cda1f997e80c95232c"
      }

in    λ(toolset : Text)
    → ../completion/wrapper
      { toolset =
          toolset
      , expression =
          "${script.url} ${script.hash} as Text"
      , command =
          "bazel"
      }

-- vim: filetype=dhall
