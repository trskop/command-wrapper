let script =
      { url =
          "https://raw.githubusercontent.com/trskop/command-wrapper/master/dhall/Exec/completion/docker-compose.bash"
      , hash =
          "sha256:d922d0ebd30b13c55518ba905b0a0250b72ab41778f5196a8b1023ad2f5a2a3a"
      }

in    λ(toolset : Text)
    → ../completion/wrapper
      { toolset =
          toolset
      , expression =
          "${script.url} ${script.hash} as Text"
      , command =
          "docker-compose"
      }

-- vim: filetype=dhall
