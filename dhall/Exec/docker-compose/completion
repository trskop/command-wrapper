let script =
      { url =
          "https://raw.githubusercontent.com/trskop/command-wrapper/master/dhall/Exec/completion/docker-compose.bash"
      , hash =
          "sha256:053665e49ce078f5c0c3fda44f3f616cc1ba3885bb224f93a5459fcb4b375513"
      }

in    λ(toolset : Text)
    → ../completion/wrapper
      { toolset =
          toolset
      , expression =
          "${script.url} ${script.hash} as Text"
      , command =
          "docker-compose"
      }

-- vim: filetype=dhall
