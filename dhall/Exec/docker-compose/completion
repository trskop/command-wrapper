-- vim: filetype=dhall

let script =
      { url =
          "https://raw.githubusercontent.com/trskop/command-wrapper/f1d0363570f96f476ec68366f10f742d2702e845/dhall/Exec/completion/docker-compose.bash"
      , hash =
          "sha256:d922d0ebd30b13c55518ba905b0a0250b72ab41778f5196a8b1023ad2f5a2a3a"
      }

in    λ(toolset : Text)
    → ../completion/wrapper
        { toolset = toolset
        , expression = "${script.url} ${script.hash} as Text"
        , command = "docker-compose"
        }
