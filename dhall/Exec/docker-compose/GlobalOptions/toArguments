-- vim: filetype=dhall

let List/map =
      https://prelude.dhall-lang.org/List/map sha256:dd845ffb4568d40327f2a817eb42d1c6138b929ca758d50bc33112ef3c885680

let GlobalOptions =
        ./Type sha256:53cba1247c3b9afd5a2a200a7ee7b819b7b7180a109fa8d46258dbc1ee496853
      ? ./Type

in    λ(options : GlobalOptions)
    → List/map Text Text (λ(_ : Text) → "--file=${_}") options.files
