-- vim: filetype=dhall

let script =
        ../completion/scripts.dhall sha256:f3454ea6de5c48a64c6c3187e0f2b02452c087b173a56bce53ed17b5238ce908
      ? ../completion/scripts.dhall

let wrapper =
        ../completion/wrapper sha256:6f4115a040ee81275a40a8785baefbe391cf67cb57e5d4e79a8e0fedf572c036
      ? ../completion/wrapper

in    λ(toolset : Text)
    → wrapper
        { toolset = toolset
        , expression = "(${script.url} ${script.hash}).go-jira"
        , command = "jira"
        }
