-- vim: filetype=dhall

let DockerGlobalOptions
    : Type
    =   ./DockerGlobalOptions sha256:88e6fa8fe07e5d00621b8c6de87f845d885fd4c2fc9be493cdd8a66ddfa7f524
      ? ./DockerGlobalOptions

in    λ(globalOptions : DockerGlobalOptions)
    →   List/fold
          Text
          globalOptions.host
          (List Text)
          (   λ(host : Text)
            → λ(options : List Text)
            → options # [ "--host", host ]
          )
          ([] : List Text)
      # Optional/fold
          Text
          globalOptions.logLevel
          (List Text)
          (λ(config : Text) → [ "--config", config ])
          ([] : List Text)
      # Optional/fold
          Text
          globalOptions.config
          (List Text)
          (λ(config : Text) → [ "--config", config ])
          ([] : List Text)
