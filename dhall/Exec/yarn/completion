-- vim: filetype=dhall

let script =
        ../completion/scripts.dhall sha256:f177a97cf61d93302fc3c0454b654269d0ad3938e8aaa1c80de74acfa7d8cce4
      ? ../completion/scripts.dhall

let wrapper =
        ../completion/wrapper sha256:6f4115a040ee81275a40a8785baefbe391cf67cb57e5d4e79a8e0fedf572c036
      ? ../completion/wrapper

in    λ(toolset : Text)
    → wrapper
        { toolset = toolset
        , expression = "(${script.url} ${script.hash}).yarn"
        , command = "yarn"
        }
