let Command : Type = ./Command

let CommandWithEnvironment : Type = ./CommandWithEnvironment

let CdConfig : Type =
      { directories : List Text

      , menuTool
          : ∀(query : Optional Text)
          → CommandWithEnvironment

      -- When specified as `None Text` system shell is used.
      , shell : Optional Text

      , terminalEmulator
          : ∀(directory : Text)
          → ∀(command : Optional Command)
          → CommandWithEnvironment
      }

in  CdConfig

-- vim: filetype=dhall
