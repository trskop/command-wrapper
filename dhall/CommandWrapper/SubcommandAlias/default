-- vim: filetype=dhall

let SubcommandAlias =
        ./Type sha256:da075695e700d78e164ef85d7f28d87b7a2e4c624b5bf62cd1e35c0191280d40
      ? ./Type

let default = { description = None Text, arguments = [] : List Text }

let consistency =
        assert
      :   { Type = SubcommandAlias, default = default }::{
          , alias = "echo"
          , command = "echo"
          }
        ≡ (default ⫽ { alias = "echo", command = "echo" })

in  default
