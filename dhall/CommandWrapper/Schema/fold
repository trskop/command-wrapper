-- vim: filetype=dhall

let Schema =
        ./Type sha256:9c43cff6b1099f18d662f6a9cf8dc7c9794ee3feb0a04f6c775d64ac2d3ace11
      ? ./Type

let foldSchema
    :   ∀(r : Type)
      → ∀(handler : { Http : r, Https : r })
      → ∀(schema : Schema)
      → r
    =   λ(r : Type)
      → λ(handler : { Http : r, Https : r })
      → λ(schema : Schema)
      → merge handler schema

in  foldSchema
